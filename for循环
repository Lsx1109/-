int main()
{
	int i = 0;//初始化
	while (i<10)//判断
	{
		i++;//调整
	}
	return 0;
}



int main()
{
	int i = 0;
	//   初始化  判断    调整
	for (i = 1; i <= 10; i++)
	{
		printf("%d\n", i);
	}
	return 0;
}


int main()
{
	int i = 0;
	for (i = 1; i <= 10; i++)
	{
		if (i == 5)
			continue;
		printf("%d\n", i);
	}
	return 0;
}
